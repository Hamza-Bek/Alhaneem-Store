@code {
    private IEnumerable<OrderDto> orders;
    private bool dataLoaded = false;

    protected override async Task OnAfterRenderAsync(bool firstRender)
    {
        await JS.InvokeVoidAsync("setupActionMenus");        
    }

    protected override async Task OnInitializedAsync()
    {
        orders = await OrderAdminService.GetOrdersDeliveringAsync();
        dataLoaded = true;
    }

    private async Task OrderShipped(DeliveryStatus status, Guid orderId)
    {
        await OrderAdminService.UpdateDeliveryStatusAsync(orderId, status);
        await JS.InvokeVoidAsync("showToast", "Order is Shipped", "success");

        orders = orders.Where(o => o.Id != orderId).ToList();
        StateHasChanged();
    }
}